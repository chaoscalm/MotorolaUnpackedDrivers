[motandroidusb.Dev.NT]
CopyFiles=motandroidusb.Files.Ext

[motandroidusb.Dev.NT.Services]
Addservice = motandroidusb, 0x00000002, motandroidusb.AddService

[motandroidusb.AddService]
DisplayName    = %motandroidusb.SvcDesc%
ServiceType    = 1                  ; SERVICE_KERNEL_DRIVER
StartType      = 3                  ; SERVICE_DEMAND_START
ErrorControl   = 1                  ; SERVICE_ERROR_NORMAL
ServiceBinary  = %10%\System32\Drivers\motoandroid.sys
AddReg         = motandroidusb.AddReg
LoadOrderGroup = Base

[motandroidusb.AddReg]
HKR,"Parameters","MaximumTransferSize",0x10001,4096
HKR,"Parameters","DebugLevel",0x10001,2
HKR, Parameters\Wdf, VerboseOn,       0x00010001, 1
HKR, Parameters\Wdf, VerifierOn,      0x00010001, 1
HKR, Parameters\Wdf, DbgBreakOnError, 0x00010001, 1

[motandroidusb.Files.Ext]
motoandroid.sys

[SourceDisksNames]
1=%Disk_Description%,,,

[SourceDisksFiles]
motoandroid.sys = 1

;-------------- WDF Coinstaller installation
[DestinationDirs]
CoInstaller_CopyFiles = 11

[motandroidusb.Dev.NT.CoInstallers]
AddReg=CoInstaller_AddReg
CopyFiles=CoInstaller_CopyFiles

[CoInstaller_CopyFiles]
wdfcoinstaller01009.dll

[SourceDisksFiles]
wdfcoinstaller01009.dll=1 ; make sure the number matches with SourceDisksNames

[CoInstaller_AddReg]
HKR,,CoInstallers32,0x00010000, "wdfcoinstaller01009.dll,WdfCoInstaller"

[motandroidusb.Dev.NT.Wdf]
KmdfService = motandroidusb, motandroidusb_wdfsect

[motandroidusb_wdfsect]
KmdfLibraryVersion = 1.9

