; Model sections ---------------------------------------------------------------

[motusbdev.NT]
CopyFiles     = CopyFileSection

[motusbdev.NT.HW]
AddReg        = motusbdevaddreg

[motusbdev.svc.NT]
CopyFiles     = CopyFileSection

[motusbdev.svc.NT.HW]
AddReg        = motusbdevaddreg_svc

[motusbdev.noswch.NT]
CopyFiles     = CopyFileSection

[motusbdev.noswch.NT.HW]
AddReg        = motusbdevaddreg_noswch


[motusbdevaddreg]
HKR,,"DelayedTime",0x00010001,0x3e8

[motusbdevaddreg_svc]
HKR,,"MotSvcEnabled",0x00010001,0x1
HKR,,"DelayedTime",0x00010001,0x3e8

[motusbdevaddreg_noswch]
HKR,,"SwitchDisable",0x00010001,0x1
HKR,,"DelayedTime",0x00010001,0x3e8

; Services ---------------------------------------------------------------------

[motusbdev.NT.Services]
AddService    = motusbdevice,   2, motusbdev_Service_Inst

[motusbdev.svc.NT.Services]
AddService    = motusbdevice,   2, motusbdev_Service_Inst

[motusbdev.noswch.NT.Services]
AddService    = motusbdevice,   2, motusbdev_Service_Inst

; Service installation ---------------------------------------------------------
[motusbdev_Service_Inst]
DisplayName   = Motorola USB Dev Driver
ServiceType   = 1
StartType     = 3
ErrorControl  = 0
ServiceBinary = %12%\motusbdevice.sys


; Coinstaller installation -----------------------------------------------------
 
[DestinationDirs]
motusbdev_CoInstaller_CopyFiles = 11

[motusbdev.NT.CoInstallers]
AddReg     = motusbdev_CoInstaller_AddReg
CopyFiles  = motusbdev_CoInstaller_CopyFiles

[motusbdev.svc.NT.CoInstallers]
AddReg     = motusbdev_CoInstaller_AddReg
CopyFiles  = motusbdev_CoInstaller_CopyFiles

[motusbdev.noswch.NT.CoInstallers]
AddReg     = motusbdev_CoInstaller_AddReg
CopyFiles  = motusbdev_CoInstaller_CopyFiles

[motusbdev_CoInstaller_AddReg]
HKR,,CoInstallers32,0x00010000, "wdfcoinstaller01009.dll,WdfCoInstaller"

[motusbdev_CoInstaller_CopyFiles]
wdfcoinstaller01009.dll

[motusbdev.NT.Wdf]
KmdfService        = motusbdevice,   motusbdev_wdfsect

[motusbdev.svc.NT.Wdf]
KmdfService        = motusbdevice,   motusbdev_wdfsect

[motusbdev.noswch.NT.Wdf]
KmdfService        = motusbdevice,   motusbdev_wdfsect

[motusbdev_wdfsect]
KmdfLibraryVersion = 1.9

; Strings ----------------------------------------------------------------------

[Strings]
Motorola             = "Motorola"
motusbdevice.desc    = "Motorola USB Device"
