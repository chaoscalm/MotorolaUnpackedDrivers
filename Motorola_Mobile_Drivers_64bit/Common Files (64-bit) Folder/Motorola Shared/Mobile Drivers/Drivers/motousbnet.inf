; Service installation ---------------------------------------------------------

[MotoSwitch_Service_Inst]
DisplayName     = "MotoSwitch Service"
ServiceType     = 1 ;%SERVICE_KERNEL_DRIVER%
StartType       = 3 ;%SERVICE_DEMAND_START%
ErrorControl    = 0 ;%SERVICE_ERROR_IGNORE%
ServiceBinary   = %12%\motswch.sys
LoadOrderGroup  = extended base

[BTC_Filter_Service_Inst]
DisplayName     = "USB Networking Driver Filter Service"
ServiceType     = 1    ; SERVICE_KERNEL_DRIVER
StartType       = 3    ; SERVICE_AUTO_START
ErrorControl    = 0    ; NORMAL
ServiceBinary   = %12%\motfilt.sys
LoadOrderGroup  = PNP Filter

[Motousbnet.Service]
DisplayName     = %Motousbnet.Service.DispName%
ServiceType     = 1 ;%SERVICE_KERNEL_DRIVER%
StartType       = 3 ;%SERVICE_DEMAND_START%
ErrorControl    = 1 ;%SERVICE_ERROR_NORMAL%
ServiceBinary   = %12%\Motousbnet.sys
LoadOrderGroup  = NDIS
AddReg          = TextModeFlags.Reg

[Motousbnet.EventLog]
AddReg = Motousbnet.AddEventLog.Reg

[Motousbnet.AddEventLog.Reg]
HKR, , EventMessageFile, 0x00020000, "%%SystemRoot%%\System32\netevent.dll"
HKR, , TypesSupported,   0x00010001, 7

[TextModeFlags.Reg]
HKR, , TextModeFlags,    0x00010001, 0x0001

; Miniport common --------------------------------------------------------------

[Motousbnet.Reg]
HKR,    ,                         BusNumber,           0, "0"
HKR, Ndi,                         Service,             0, "Motousbnet"
HKR, Ndi\Interfaces,              UpperRange,          0, "ndis5"
HKR, Ndi\Interfaces,              LowerRange,          0, "ethernet"

[ParamsPromiscuous]
;
;    Should the physical NIC be set to Promiscuous mode
;
HKR, Ndi\Params\Promiscuous,     ParamDesc, , %Promiscuous%
HKR, Ndi\Params\Promiscuous,     Default,  ,"0"
HKR, Ndi\Params\Promiscuous,     type, ,      enum
HKR, Ndi\Params\Promiscuous\enum,"1",  ,     %Promiscuous_Enable%
HKR, Ndi\Params\Promiscuous\enum,"0",  ,     %Promiscuous_Disable%

