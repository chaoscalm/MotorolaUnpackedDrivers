AddReg    = USBMOTPORT.NT.AddReg


[MOTPORT1.NT]
CopyFiles = CopyFileSection
AddReg    = USBMOTPORT.NT.AddReg

[MOTPORT1.NT.HW]
AddReg    = MOTPORT1.NT.AddReg


[USBMOTPORT.NT.AddReg]
HKR,,PortSubClass,1,01
HKR,,EnumPropPages32,,"MsPorts.dll,SerialPortPropPageProvider"

[MOTPORT1.NT.AddReg]
HKR,,"MotConfigFlag",0x00010001,1


; Services ---------------------------------------------------------------------

[USBMOTPORT.NT.Services]
AddService = motport,0x00000002,MotPort_Service_Inst

[MOTPORT1.NT.Services]
AddService = motport,0x00000002,MotPort_Service_Inst


; Service installation ---------------------------------------------------------

[MotPort_Service_Inst]
DisplayName   = Motorola USB Diagnostic Port
ServiceType   = 1
StartType     = 3
ErrorControl  = 0
ServiceBinary = %12%\motport.sys

; Coinstaller installation -----------------------------------------------------
 
[DestinationDirs]
USBMOTPORT_CoInstaller_CopyFiles = 11

[USBMOTPORT.NT.CoInstallers]
AddReg        = USBMOTPORT_CoInstaller_AddReg
CopyFiles     = USBMOTPORT_CoInstaller_CopyFiles

[MOTPORT1.NT.CoInstallers]
AddReg        = USBMOTPORT_CoInstaller_AddReg
CopyFiles     = USBMOTPORT_CoInstaller_CopyFiles
[USBMOTPORT_CoInstaller_AddReg]
HKR,,CoInstallers32,0x00010000, "wdfcoinstaller01009.dll,WdfCoInstaller"

[USBMOTPORT_CoInstaller_CopyFiles]
wdfcoinstaller01009.dll

[SourceDisksFiles]
wdfcoinstaller01009.dll = 99

[USBMOTPORT.NT.Wdf]
KmdfService = motport, motport_wdfsect

[MOTPORT1.NT.Wdf]
KmdfService = motport, motport_wdfsect

[motport_wdfsect]
KmdfLibraryVersion = 1.9

; Strings ----------------------------------------------------------------------

[Strings]
MotDisk       = "Motorola USB Diagnostic Port Install Disk" 
USBMOTPORT    = "Motorola USB Diagnostic Port"
USBMOTPORTLX  = "Motorola USB Linux Diagnostic Port"
USBMOTPORTAP  = "Motorola USB AP Diagnostic Port"
USBMOTPORTBP  = "Motorola USB BP Diagnostic Port"
USBNETPORT    = "Motorola USB Network Monitor Device"
USBATPORT     = "Motorola AT COM Port"
USBTRACEPORT  = "Motorola Trace COM Port"
ServiceName   = "motport"
Motorola      = "Motorola"
USBCONPORT    = "Motorola Connectivity Port"
USBCHRGPORT   = "Motorola Charging Port"
USBMOTGPSPORT = "Motorola USB GPS Port"
USBMOTQCDIAGPORT = "Motorola QC Diag Port"
